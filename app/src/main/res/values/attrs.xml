<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WeekDayView">
        <attr name="wdv_day_number" format="enum">
            <enum name="monday" value="2"/>
            <enum name="tuesday" value="3"/>
            <enum name="wednesday" value="4"/>
            <enum name="thursday" value="5"/>
            <enum name="friday" value="6"/>
            <enum name="saturday" value="7"/>
            <enum name="sunday" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LoopView">
        <attr name="lineColor" format="color"/>
        <attr name="topBottomTextColor" format="color"/>
        <attr name="centerTextColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="canLoop" format="boolean"/>
        <attr name="initPosition" format="integer"/>
        <attr name="drawItemCount" format="integer"/>
    </declare-styleable>

</resources>